<?xml version="1.0" encoding="utf-8"?>
<manifest package="imageviewer.eoinahern.ie.imageviewer"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<uses-permission android:name="android.permission.INTERNET"></uses-permission>

	<application
		android:name=".App"
		android:allowBackup="true"
		android:icon="@drawable/fc2_logo_red_rounded"
		android:label="@string/app_name"

		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".ui.view.splash.SplashActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name=".ui.view.login.LoginActivity"
			android:screenOrientation="portrait"></activity>
		<activity
			android:name=".ui.view.selection.SelectionActivity"
			android:screenOrientation="portrait"></activity>
		<activity
			android:name=".ui.view.detail.DetailActivity"
			android:screenOrientation="portrait"></activity>

		<meta-data
			android:name="preloaded_fonts"
			android:resource="@array/preloaded_fonts" />
	</application>

</manifest>